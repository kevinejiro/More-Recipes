{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "A platform for users to share their recipe ideas with the world",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build:front": "webpack --config ./webpack.config.prod.js",
    "start:client": "webpack-dev-server --open --config ./webpack.config.dev.js",
    "start:dev": "nodemon --exec babel-node server/bin/www",
    "test:local": "cross-env NODE_ENV=test_local&& npm run migrate:undo&& npm run migrate&& nyc mocha server/test/test.js --compilers js:babel-core/register",
    "test:db": "set NODE_ENV=test && sequelize db:migrate:undo:all&&sequelize db:migrate && cross-env NODE_ENV=test nyc mocha server/test/test.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register server/test/test.js",
    "test:client": "jest ./client --coverage",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "node ./node_modules/babel-cli/bin/babel.js -d ./dist ./server",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "migrate": "node ./node_modules/sequelize-cli/bin/sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "heroku-postbuild": "npm run build && npm run build:front && npm run migrate"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/client/__tests__/jestSetup.js"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(mp4|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/client/__tests__/__mock__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "Ejiro",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.1.10",
    "cors": "^2.8.4",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.0",
    "dotenv-webpack": "^1.5.5",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "firebase": "^4.12.0",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-firebase-file-uploader": "^2.4.1",
    "react-paginate": "^5.2.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0-beta",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.8.0",
    "sequelize-cli": "^2.8.0",
    "style-ext-html-webpack-plugin": "^3.4.6",
    "style-loader": "^0.19.1",
    "svg-inline-loader": "^0.8.0",
    "sweetalert2": "^7.17.0",
    "toastr": "^2.1.4",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.11.0",
    "webpack-hot-middleware": "^2.21.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "redux-mock-store": "^1.5.1",
    "sinon": "^4.2.2",
    "supertest": "^3.0.0"
  }
}
